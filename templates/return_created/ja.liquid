                                  <th class="section_content" style="mso-line-height-rule: exactly; padding: 8px 30px;" bgcolor="#fefcf6" valign="top">{% for return_delivery in return.deliveries %} {% if return_delivery.type == "shopify_label" %} {% else %} {% if return_delivery.tracking_url != blank %} {% endif %} {% endif %} {% endfor %}
                                  <table cellspacing="0" cellpadding="0" border="0" width="100%" style="direction: ltr;">
