                          <tr id="section-26492753" class="section order_number_and_date">
                            <th class="section_border" style="mso-line-height-rule: exactly; padding: 8px 30px;" bgcolor="#fefcf6">
                              <table border="0" width="100%" cellpadding="0" cellspacing="0" align="center" style="min-width: 100%; direction: ltr;" role="presentation">
                                <tr>
                                  <th class="section_content" style="mso-line-height-rule: exactly; padding: 8px 30px;" bgcolor="#fefcf6" valign="top">
                                    {% if name != blank %}
                                      <h2 style="font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; color: #2e2a39; font-size: 20px; line-height: 34px; font-weight: 500; text-transform: none; letter-spacing: 1.5px; Margin: 0;" align="left">
                                        <span data-key="26492753_order_number">ご注文番号：</span> {{ name }}
                                      </h2>

                                      <p class="muted" style="mso-line-height-rule: exactly; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; Margin: 0;" align="left"> {% include 'date_jp.liquid' with  created_at %}</p>
                                    {% endif %}
                                  </th>
                                </tr>
                              </table>
                            </th>
                          </tr>
                          <!-- END SECTION: Order Number And Date -->
                          <!-- BEGIN SECTION: Products With Pricing -->
                          <tr id="section-26492754" class="section products_with_pricing">
                            <th class="section_border" style="mso-line-height-rule: exactly; padding: 0 30px;" bgcolor="#fefcf6">
                              <table border="0" width="100%" cellpadding="0" cellspacing="0" align="center" style="min-width: 100%; direction: ltr;" role="presentation">
                                <tr>
                                  {% if line_items != blank %}
                                    <!-- Bold 1 -->
                                    {% assign hidden_variant_ids = "" %}
                                    {% for line_item in line_items %}
                                      {% if line_item.properties._boldVariantIds %}
                                        {% assign hidden_variant_ids = hidden_variant_ids | append: ',' %}
                                        {% assign hidden_variant_ids = hidden_variant_ids | append: line_item.properties._boldVariantIds %}
                                      {% endif %}
                                    {% endfor %}
                                    <!-- end Bold 1 -->
                                    <th class="section_content" style="mso-line-height-rule: exactly; border-radius: 24px; padding: 30px; border: #fefcf6;" bgcolor="#fefcf6" valign="top">
                                      <table class="table_inner" cellspacing="0" cellpadding="0" border="0" width="100%" style="min-width: 100%; direction: ltr; border: #fefcf6;" role="presentation" bgcolor="#fefcf6">
                                        <tr style="border: #fefcf6;" bgcolor="#fefcf6">
                                          <th class="product_table  table_content" style="mso-line-height-rule: exactly; padding-top: 8px; padding-bottom: 8px; border: #fefcf6;" bgcolor="#fefcf6" valign="top">
                                            <table cellspacing="0" cellpadding="0" border="0" width="100%" style="min-width: 100%; direction: ltr; border: #fefcf6;" role="presentation" bgcolor="#fefcf6">
                                              <tr style="border: #fefcf6;" bgcolor="#fefcf6">
                                                <th colspan="2" class="table_main_title" style="mso-line-height-rule: exactly; border: #fefcf6;" bgcolor="#fefcf6" valign="top">
                                                  <h3 data-key="26492754_item" style="font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; color: #2e2a39; font-size: 18px; line-height: 34px; font-weight: 500; text-transform: none; border-bottom-width: 1px; border-bottom-style: solid; letter-spacing: 1.5px; background-color: #fefcf6 !important; Margin: 0; border: #fefcf6;" align="left">お取り置き商品</h3>
                                                </th>
                                              </tr>
                                              {% for line_item in line_items %}
                                                {% if line_item.quantity != 0 or line_item.product.vendor != 'Route' %}
                                                  <!-- Bold 2 -->
                                                  {% capture line_variant_id1 %}{{ line_item.variant_id }}{% endcapture %}
                                                  {% assign adjusted_item_price = line_item.price | default: 0 %}
                                                  {% assign adjusted_line_price = line_item.line_price | default: 0 %}
                                                  {% assign adjusted_line_option_price = 0 %}
                                                  {% unless line_variant_id1 == blank %}
                                                    {% if hidden_variant_ids contains line_variant_id1 %}{% continue %}{% endif %}
                                                    {% for line_item2 in line_items %}
                                                      {% capture line_variant_id2 %}{{ line_item2.variant_id }}{% endcapture %}
                                                      {% if line_item.properties._boldVariantIds contains line_variant_id2 and line_item.properties._boldBuilderId == line_item2.properties._boldBuilderId %}
                                                        {% assign adjusted_item_price = adjusted_item_price | plus: line_item2.price %}
                                                        {% assign adjusted_line_option_price = line_item2.line_price %}
                                                        {% assign adjusted_line_price = adjusted_line_price | plus: adjusted_line_option_price %}
                                                      {% endif %}
                                                    {% endfor %}
                                                  {% endunless %}
                                                  <!-- end Bold 2 -->
                                                  <tr class="row-border-bottom" style="border: #fefcf6;" bgcolor="#fefcf6">
                                                    <th class="table-stack product-image-wrapper stack-column-center" width="1" style="mso-line-height-rule: exactly; border-bottom-width: 1px; border-bottom-style: none; padding-top: 15px; padding-bottom: 15px; padding-right: 15px; border: #fefcf6;" bgcolor="#fefcf6" valign="middle">{% if line_item.image != blank %}<a href="{% capture link %}https://{{shop.domain}}{{line_item.product.url}}{% endcapture %}{% if link contains shop.domain %}{% assign split_link = link | split: question_mark %}{{ split_link[0] }}?{{ default_utms }}&utm_content=product{% if split_link[1] != blank %}&{{ split_link[1] }}{% endif %}{% else %}{{ link }}{% endif %}" class="product_link" target="_blank" style="color: #000000 !important; text-decoration: none !important; text-underline: none; word-wrap: break-word; border-bottom-width: 1px; border-bottom-color: #5f5f5f; border: none;"><img width="160" class="product-image" src="{{ line_item.image | image_url: width: 320 }}" alt="{{ line_item.title }}" style="background-color: #fefcf6 !important; vertical-align: middle; text-align: center; width: 160px; max-width: 160px; height: auto !important; border-radius: 12px; padding: 0px; border: #fefcf6;"></a>{% endif %}</th>
                                                    <th class="product-details-wrapper table-stack stack-column" style="mso-line-height-rule: exactly; border-bottom-width: 1px; border-bottom-style: none; padding-top: 15px; padding-bottom: 15px; border: #fefcf6;" bgcolor="#fefcf6" valign="middle">
                                                      <table cellspacing="0" cellpadding="0" border="0" width="100%" style="min-width: 100%; direction: ltr; border: #fefcf6;" role="presentation" bgcolor="#fefcf6">
                                                        <tr style="border: #fefcf6;" bgcolor="#fefcf6">
                                                          {% capture line_title %}{{ line_item.product.title | default: line_item.title }}{% endcapture %}
                                                          <th class="line-item-description" style="mso-line-height-rule: exactly; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 15px; line-height: 30px; font-weight: 300; text-transform: none; color: #000000; padding: 15px 8px 15px 0; border: #fefcf6;" align="left" bgcolor="#fefcf6" valign="top">
                                                            <p style="mso-line-height-rule: exactly; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 15px; line-height: 30px; font-weight: 300; text-transform: none; color: #000000; background-color: #fefcf6 !important; Margin: 0; border: #fefcf6;" align="left">
                                                              <a href="{% capture link %}https://{{shop.domain}}{{line_item.product.url}}{% endcapture %}{% if link contains shop.domain %}{% assign split_link = link | split: question_mark %}{{ split_link[0] }}?{{ default_utms }}&utm_content=product{% if split_link[1] != blank %}&{{ split_link[1] }}{% endif %}{% else %}{{ link }}{% endif %}" class="product_link" target="_blank" style="color: #000000 !important; text-decoration: none !important; text-underline: none; word-wrap: break-word; border-bottom-width: 1px; border-bottom-color: #5f5f5f; font-weight: 300; text-align: left !important; border: none;">
                                                                {% if line_title == "Tip" %}Tip{% else %}{{ line_title }}{% endif %}
                                                                </a>
                                                                {% if line_item.variant.title != blank and line_item.variant.title != "Default Title" and line_item.variant.title != "Default" %}
                                                                  <br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                  <span class="muted" style="text-align: left; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; background-color: #fefcf6 !important; border: #fefcf6;">{{ line_item.variant.title }}</span>
                                                                {% endif %}
                                                                {% if line_item.selling_plan_allocation != blank %}
                                                                  <br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                  <span class="muted" style="text-align: left; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; background-color: #fefcf6 !important; border: #fefcf6;">{{ line_item.selling_plan_allocation.selling_plan.name }}</span>
                                                                {% endif %}
                                                                {% for p in line_item.properties %}
                                                                  {% assign p_internal = p.first | slice: 0 %}
                                                                  {% unless p.first contains "builder_id" or p.first contains "builder_info" or p.first contains "master_builder" or p.last == "" or p.last == blank %}
                                                                    {% if p.first contains "preview" or p.last contains "/uploads/" or p.last contains "//uploadery.s3" %}
                                                                      <br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                      <span class="muted" style="text-align: left; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; background-color: #fefcf6 !important; border: #fefcf6;">{{ p.first }}:<br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                      </span>
                                                                      <a href="{{ p.last }}" target="_blank" style="color: #000000 !important; text-decoration: none !important; text-underline: none; word-wrap: break-word; border-bottom-width: 1px; border-bottom-color: #5f5f5f; font-weight: 300; text-align: left !important; border: none;"><img src="{{ p.last }}" alt="{{ p.first }}" width="60" border="0" style="width: 60px; height: auto !important; background-color: #fefcf6 !important; border: #fefcf6;"></a>
                                                                    {% elsif p_internal != "_" %}
                                                                      <br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                      <span class="muted" style="text-align: left; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; background-color: #fefcf6 !important; border: #fefcf6;">{{ p.first }}: {{ p.last }}</span>
                                                                    {% endif %}
                                                                  {% endunless %}
                                                                {% endfor %}
                                                                {% if line_item.variant.unit_price_measurement != blank %}
                                                                  <br style="background-color: #fefcf6 !important; border: #fefcf6;">
                                                                  <span class="muted" style="text-align: left; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 13px; line-height: 30px; font-weight: normal; text-transform: none; color: #828a91; background-color: #fefcf6 !important; border: #fefcf6;">({{ line_item.variant.unit_price | money }}{% if line_item.variant.unit_price_measurement.reference_value != 1 %} {{ line_item.variant.unit_price_measurement.reference_value }}{% endif %}/{{ line_item.variant.unit_price_measurement.reference_unit | capitalize }})</span>
                                                                {% endif %}
                                                              </p>
                                                            </th>
                                                            <th class="right line-item-qty" width="1" style="mso-line-height-rule: exactly; white-space: nowrap; padding: 15px 8px 15px 30px; border: #fefcf6;" align="right" bgcolor="#fefcf6" valign="top">
                                                              <p style="mso-line-height-rule: exactly; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 15px; line-height: 30px; font-weight: 300; text-transform: none; color: #000000; background-color: #fefcf6 !important; Margin: 0; border: #fefcf6;" align="right">x&nbsp;{{ line_item.quantity }}</p>
                                                            </th>
                                                            <th class="right line-item-line-price" width="1" style="mso-line-height-rule: exactly; white-space: nowrap; padding: 15px 0 15px 30px; border: #fefcf6;" align="right" bgcolor="#fefcf6" valign="top">
                                                              <p style="mso-line-height-rule: exactly; direction: ltr; font-family: -apple-system,BlinkMacSystemFont,'Segoe UI',Arial,'Poppins'; font-size: 15px; line-height: 30px; font-weight: 300; text-transform: none; color: #000000; background-color: #fefcf6 !important; Margin: 0; border: #fefcf6;" align="right">
                                                                {{ adjusted_line_price | money }}
                                                              </p>
                                                            </th>
